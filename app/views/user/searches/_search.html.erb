<%= form_with url: user_search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content, size: '40x50' %>
    <%= f.select :model, options_for_select({ "バイク" => "item", "ユーザー" => "user" }) %>
    <%= f.select :method, options_for_select({ "完全一致" => "perfect", "部分一致" => "partial" }) %>
    <%= f.submit '検索', class: 'btn btn-primary' %>
  <% end %>