<div class='container'>
    <div class='text-center'>
        <h2>配送先編集</h2>
    </div>
    <%= form_with model: @address, url: user_address_path do |f| %>
    <%= f.label :郵便番号（ハイフンなし） %>
    <%= f.text_field :postal_code %>
        
    <%= f.label :住所 %>
    <%= f.text_field :address %>
        
    <%= f.label :宛名 %>
    <%= f.text_field :name %>
    <%= f.submit "変更内容を保存", class: 'btn btn-success' %>
    <% end %>
</div>


